<%= form_for(@place) do |f| %>
  <% if @place.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@place.errors.count, "error") %> prohibited this venue from being saved:</h2>
      <ul>
      <% @place.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="actions">
    <p><%= f.label :name %><%= f.text_field :name, class: 'form-control' %></p>
    <p><%= f.label :address %><%= f.text_field :address, class: 'form-control' %></p>
    <p><%= f.label :city %><%= f.text_field :city, class: 'form-control' %></p>
    <p><%= f.label :state %><%= f.text_field :state, class: 'form-control' %></p>
    <%= f.submit value="Save", class: 'btn btn-success' %>
    <%= link_to 'Back', acts_path, class: 'btn btn-info'  %>
  </div>
<% end %>
